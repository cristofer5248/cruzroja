	</parameter>
	<queryString>
		<![CDATA[SELECT
     datosbeneficiados.`nombres` AS datosbeneficiados_nombres,
     datosbeneficiados.`apellidos` AS datosbeneficiados_apellidos,
     datosbeneficiados.`edad` AS datosbeneficiados_edad,
     lugar.`nombre` AS lugar_nombre,
     proyectos.`nombre` AS proyectos_nombre,
     departamento.`nombre` AS departamento_nombre,
     usuarios.`nombres` AS usuarios_nombres,
     genero.`nombre` AS genero_nombre
FROM
     `datosbeneficiados` datosbeneficiados INNER JOIN `beneficiados` beneficiados ON datosbeneficiados.`idusuario` = beneficiados.`idbeneficiado`
     INNER JOIN `lugarproyecto` lugarproyecto ON beneficiados.`idproyecto` = lugarproyecto.`idl`
     INNER JOIN `lugar` lugar ON lugarproyecto.`idlugar` = lugar.`idlugar`
     INNER JOIN `proyectos` proyectos ON lugarproyecto.`idlp` = proyectos.`idproyecto`
     INNER JOIN `usuarios` usuarios ON proyectos.`encargado` = usuarios.`idusuario`
     INNER JOIN `genero` genero ON usuarios.`genero` = genero.`idg`
     AND genero.`idg` = datosbeneficiados.`genero`
     INNER JOIN `departamento` departamento ON lugar.`departamento` = departamento.`iddepartamento`
WHERE
     beneficiados.`idbeneficiado` = $P{codbene}]]>
	</queryString>
