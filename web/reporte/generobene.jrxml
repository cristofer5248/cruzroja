	<queryString language="SQL">
		<![CDATA[SELECT
     datosbeneficiados.`nombres` AS datosbeneficiados_nombres,
     datosbeneficiados.`apellidos` AS datosbeneficiados_apellidos,
     datosbeneficiados.`correo` AS datosbeneficiados_correo,
     datosbeneficiados.`edad` AS datosbeneficiados_edad,
     datosbeneficiados.`telefono` AS datosbeneficiados_telefono,
     proyectos.`nombre` AS proyectos_nombre,
     genero.`nombre` AS genero_nombre
FROM
     `proyectos` proyectos INNER JOIN `lugarproyecto` lugarproyecto ON proyectos.`idproyecto` = lugarproyecto.`idlp`
     INNER JOIN `beneficiados` beneficiados ON lugarproyecto.`idl` = beneficiados.`idproyecto`
     INNER JOIN `datosbeneficiados` datosbeneficiados ON beneficiados.`idbeneficiado` = datosbeneficiados.`idusuario`
     INNER JOIN `genero` genero ON datosbeneficiados.`genero` = genero.`idg`]]>
	</queryString>
