	<queryString language="SQL">
		<![CDATA[SELECT
     departamento.`nombre` AS departamento_nombre,
     actividadesdetalles.`titulo` AS actividadesdetalles_titulo,
     lugarcategoria.`nombre` AS lugarcategoria_nombre,
     lugar.`nombre` AS lugar_nombre,
     actividades.`idactividad` AS actividades_idactividad,
     municipio.`nombre` AS municipio_nombre
FROM
     `departamento` departamento INNER JOIN `municipio` municipio ON departamento.`iddepartamento` = municipio.`departamento`
     INNER JOIN `lugar` lugar ON municipio.`idmunicipio` = lugar.`municipio`
     AND departamento.`iddepartamento` = lugar.`departamento`
     INNER JOIN `lugarcategoria` lugarcategoria ON lugar.`categoria` = lugarcategoria.`id`
     INNER JOIN `lugarproyecto` lugarproyecto ON lugar.`idlugar` = lugarproyecto.`idlugar`
     INNER JOIN `actividades` actividades ON lugarproyecto.`idl` = actividades.`lugarproyectoPadre`
     INNER JOIN `actividadesdetalles` actividadesdetalles ON actividades.`detalleactividad` = actividadesdetalles.`idactividadesdetalles`]]>
	</queryString>
