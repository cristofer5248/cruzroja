	<queryString>
		<![CDATA[SELECT
     proyectos.`nombre` AS proyectos_nombre,
     lugar.`nombre` AS lugar_nombre,
     municipio.`nombre` AS municipio_nombre,
     categoria.`nombre` AS categoria_nombre,
     usuarios.`nombres` AS usuarios_nombres,
     lugarproyecto.`fechainicio` AS lugarproyecto_fechainicio,
     lugarproyecto.`fechafinal` AS lugarproyecto_fechafinal
FROM
     `proyectos` proyectos INNER JOIN `lugarproyecto` lugarproyecto ON proyectos.`idproyecto` = lugarproyecto.`idlp`
     INNER JOIN `lugar` lugar ON lugarproyecto.`idlugar` = lugar.`idlugar`
     INNER JOIN `lugar` lugar_A ON lugarproyecto.`idlugar` = lugar_A.`idlugar`
     INNER JOIN `municipio` municipio ON lugar_A.`municipio` = municipio.`idmunicipio`
     AND municipio.`idmunicipio` = proyectos.`seccionales`
     INNER JOIN `categoria` categoria ON proyectos.`categoria` = categoria.`idcategoria`
     INNER JOIN `usuarios` usuarios ON proyectos.`encargado` = usuarios.`idusuario`]]>
	</queryString>
