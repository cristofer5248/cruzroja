	<queryString language="SQL">
		<![CDATA[SELECT
     actividadesdetalles.`titulo` AS actividadesdetalles_titulo,
     datosbeneficiados.`nombres` AS datosbeneficiados_nombres,
     datosbeneficiados.`apellidos` AS datosbeneficiados_apellidos,
     genero.`nombre` AS genero_nombre,
     proyectos.`nombre` AS proyectos_nombre,
     datosbeneficiados.`idusuario` AS datosbeneficiados_idusuario
FROM
     `actividadesdetalles` actividadesdetalles INNER JOIN `actividades` actividades ON actividadesdetalles.`idactividadesdetalles` = actividades.`detalleactividad`
     INNER JOIN `beneficiados` beneficiados ON actividades.`idactividad` = beneficiados.`idproyecto`
     INNER JOIN `lugarproyecto` lugarproyecto ON actividades.`lugarproyectoPadre` = lugarproyecto.`idl`
