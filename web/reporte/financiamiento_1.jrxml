	<queryString language="SQL">
		<![CDATA[SELECT
     financiamiento.`financiamiento` AS financiamiento_financiamiento,
     financiamiento.`fechaIngreso` AS financiamiento_fechaIngreso,
     financiamiento.`Detalles` AS financiamiento_Detalles,
     proyectos.`nombre` AS proyectos_nombre,
     sociedad.`nombre` AS sociedad_nombre,
     categoria.`nombre` AS categoria_nombre,
     municipio.`nombre` AS municipio_nombre
FROM
     `proyectos` proyectos INNER JOIN `financiamiento` financiamiento ON proyectos.`idproyecto` = financiamiento.`proyecto`
     INNER JOIN `sociedad` sociedad ON financiamiento.`sociedadN` = sociedad.`id`
     INNER JOIN `categoria` categoria ON proyectos.`categoria` = categoria.`idcategoria`
     INNER JOIN `municipio` municipio ON proyectos.`seccionales` = municipio.`idmunicipio`]]>
	</queryString>
