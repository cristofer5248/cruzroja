	<queryString>
		<![CDATA[SELECT
     lugarproyecto.`fechainicio` AS lugarproyecto_fechainicio,
     lugarproyecto.`fechafinal` AS lugarproyecto_fechafinal,
     proyectos.`nombre` AS proyectos_nombre,
     usuarios.`nombres` AS usuarios_nombres,
     departamento.`nombre` AS departamento_nombre,
     lugar.`nombre` AS lugar_nombre,
     proyectos.`des` AS proyectos_des,
     lugarproyecto.`idl` AS lugarproyecto_idl
FROM
     `lugarproyecto` lugarproyecto INNER JOIN `beneficiados` beneficiados ON lugarproyecto.`idl` = beneficiados.`idproyecto`
     INNER JOIN `proyectos` proyectos ON lugarproyecto.`idlp` = proyectos.`idproyecto`
     INNER JOIN `usuarios` usuarios ON lugarproyecto.`encargadoproyecto` = usuarios.`idusuario`
     INNER JOIN `lugar` lugar ON lugarproyecto.`idlugar` = lugar.`idlugar`
     INNER JOIN `departamento` departamento ON lugar.`departamento` = departamento.`iddepartamento`
GROUP BY
     lugarproyecto_idl
ORDER BY
     'departamento_nombre' ASC]]>
	</queryString>
