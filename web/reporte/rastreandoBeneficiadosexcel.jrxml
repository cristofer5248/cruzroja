	<subDataset name="cruz" uuid="06ec9e67-43be-4221-8f55-97e5228eb034">
		<queryString language="SQL">
			<![CDATA[SELECT
     
     datosbeneficiados.`nombres` AS datosbeneficiados_nombres,
     datosbeneficiados.`idusuario` AS datosbeneficiados_idusuario,
     datosbeneficiados.`apellidos` AS datosbeneficiados_apellidos,
     datosbeneficiados.`edad` AS datosbeneficiados_edad,
     lugarproyecto.`fechainicio` AS lugarproyecto_fechainicio,
     lugarproyecto.`fechafinal` AS lugarproyecto_fechafinal,
     datosbeneficiados.`telefono` AS datosbeneficiados_telefono,
     genero.`nombre` AS genero_nombre,
     actividadesdetalles.`titulo` AS actividadesdetalles_titulo,
     proyectos.`nombre` AS proyectos_nombre,
     municipio.`nombre` AS municipio_nombre,
     departamento.`nombre` AS departamento_nombre
FROM
     `datosbeneficiados` datosbeneficiados INNER JOIN `beneficiados` t2 ON datosbeneficiados.`idusuario` = t2.`idbeneficiado`
     INNER JOIN `genero` genero ON datosbeneficiados.`genero` = genero.`idg`
     INNER JOIN `actividades` actividades ON t2.`idproyecto` = actividades.`idactividad`
     INNER JOIN `lugarproyecto` lugarproyecto ON actividades.`lugarproyectoPadre` = lugarproyecto.`idl`
     INNER JOIN `actividadesdetalles` actividadesdetalles ON actividades.`detalleactividad` = actividadesdetalles.`idactividadesdetalles`
     INNER JOIN `actividades` t1 ON lugarproyecto.`idl` = t1.`lugarproyectoPadre`
     INNER JOIN `proyectos` proyectos ON lugarproyecto.`idlp` = proyectos.`idproyecto`
     INNER JOIN `municipio` municipio ON proyectos.`seccionales` = municipio.`idmunicipio`
     INNER JOIN `departamento` departamento ON municipio.`departamento` = departamento.`iddepartamento`,
     `lugarproyecto` t0
WHERE
     (((CAST(t0.fechainicio AS Date) >= '2017-12-04')
 AND (CAST(t0.fechafinal AS Date) <= '2018-12-04'))
 AND ((t1.idactividad = t2.idproyecto)
 AND (t0.idl = t1.lugarproyectoPadre)))]]>
		</queryString>
		<field name="datosbeneficiados_nombres" class="java.lang.String"/>
		<field name="datosbeneficiados_idusuario" class="java.lang.String"/>
		<field name="datosbeneficiados_apellidos" class="java.lang.String"/>
		<field name="datosbeneficiados_edad" class="java.lang.Integer"/>
		<field name="lugarproyecto_fechainicio" class="java.sql.Date"/>
		<field name="lugarproyecto_fechafinal" class="java.sql.Date"/>
		<field name="datosbeneficiados_telefono" class="java.lang.Integer"/>
		<field name="genero_nombre" class="java.lang.String"/>
